{
    "presets": ["es2015", "stage-3"],
    "plugins": [
        "syntax-dynamic-import",
        "add-module-exports",
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
    ],
    "comments": false
}